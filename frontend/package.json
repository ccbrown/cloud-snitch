{
    "name": "cloud-snitch",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "bash -c 'if [ ! -z \"$ENV\" ]; then export $(cat \"$ENV\" | xargs); fi; next dev --turbopack'",
        "build": "next build",
        "check": "tsc --noEmit",
        "start": "next start",
        "lint": "next lint",
